#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    void solve(vector<vector<int>>&edges, int src, int dst) {
        vector<vector<int>>adj(8);
        for(int i=0;i<edges.size();i++) {
            int u = edges[i][0];
            int v = edges[i][1];

            adj[u].push_back(v);
            adj[v].push_back(u);
        }

        queue<int>q;
        vector<int>parents(8);
        vector<int>dist(8, INT_MAX);

        q.push(src);
        parents[src] = -1;
        dist[src] = 0;
        while(!q.empty()) {
            int size = q.size();
            while(size--) {
                int u = q.front();
                q.pop();

                for(int i=0; i<adj[u].size(); i++) {
                    int nu = adj[u][i];
                    if (dist[nu] == INT_MAX) {
                        q.push(nu);
                        parents[nu] = u;
                        dist[nu] = dist[u] + 1;
                    }
                }
            }
        }

        while(1) {
            cout<<dst<<" ";
            if (dst == -1) {
                break;
            }
            dst = parents[dst];
        }
    }
};

int main() {
    Solution* solution = new Solution();
    vector<vector<int>>edges = {
        { 0, 1 },
        { 1, 2 },
        { 0, 3 },
        { 3, 4 },
        { 4, 7 },
        { 3, 7 },
        { 6, 7 },
        { 4, 5 },
        { 4, 6 },
        { 5, 6 }
    };
    int src = 2;
    int dst = 6;

    solution->solve(edges, src, dst);
}